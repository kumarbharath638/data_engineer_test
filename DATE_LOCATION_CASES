create TABLE "INTERVIEW_DB"."PLAYGROUND_BHARATH_KUMAR"."DATE_LOCATION_CASES"
AS

select DATE, location,
sum(TOTAL_CASES) as TOTAL_CASES,
sum(TOTAL_DEATHS) as TOTAL_DEATHS,
sum(TOTAL_RECOVERED) as TOTAL_RECOVERED,
sum(TOTAL_ACTIVE_CASES) as TOTAL_ACTIVE_CASES,
from "INTERVIEW_DB"."PLAYGROUND_BHARATH_KUMAR"."COVIDDATA"
group by 1,2
order by 1,2
